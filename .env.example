# Supabase Configuration
# Get these values from your Supabase project dashboard -> Settings -> API
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Service Role Key (SERVER-SIDE ONLY)
# WARNING: Keep this secret! Never expose to client-side code
# Used by API routes for admin operations that bypass RLS
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Groq API Key (PRIMARY - used for all endpoints by default)
# Get your API key from: https://console.groq.com/keys
# Used for: Assessment generation, evaluation, and roadmap generation (default)
# Model: Llama-3.1-8B-Instant (fast, accurate, cost-effective)
GROQ_API_KEY=your_groq_api_key_here

# Google Gemini API Key (LEGACY - kept for backward compatibility)
# Get your API key from: https://ai.google.dev/ or https://aistudio.google.com/app/apikey
# Note: Current implementation uses Groq, not Gemini
GOOGLE_API_KEY=your_google_gemini_api_key_here

# Hugging Face API (OPTIONAL - Not currently used, backup option)
# Get your API key from: https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Tavily Search API (OPTIONAL)
# Get your API key from: https://tavily.com
TAVILY_API_KEY=your_tavily_api_key_here

# YouTube Data API v3 (OPTIONAL)
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Enable YouTube Data API v3 in your Google Cloud project
YOUTUBE_API_KEY=your_youtube_api_key_here

# Node Environment
# Set to 'development' for detailed error messages, 'production' for minimal logging
NODE_ENV=development
